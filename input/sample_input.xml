<?xml version="1.0"?>

<routers name="routers">
    <router id="r1" />
    <router id="r2" />
    <router id="r3" />
    <router id="r4" />
</routers>

<hosts name="hosts">
    <host id="h1" />
    <host id="h2" />
</hosts>

<links name="links">
    <link>
        <id>l1</id>
        <linkRate>10</linkRate>
        <linkDelay>1</linkDelay>
        <bufferSize>100</bufferSize>
        <router1Id>r1</router1Id>
        <router2Id>r2</router2Id>
    </link>
</links>

<flows name="flows">
    <flow>
        <id>f1</id>
        <sourceId>h1</sourceId>
        <destinationId>h2</destinationId>
        <size>1000</size>
        <flowStartTime>5</flowStartTime>
    </flow>
</flows>

<network name="network">
    <snapshotTime seconds="5" />
    <routingTableUpdateTime seconds="3" />
    <routers routers="routers" />
    <links links="links" />
    <flows flows="flows" />
</network>
