<?xml version="1.0" encoding="ISO-8859-1"?>

<Network name="thenetwork">
    <snapshotTime>3</snapshotTime>
    <routingTableUpdateTime>5</routingTableUpdateTime>
    <networkHosts>
        <host id="h1" />
        <host id="h2" />
    </networkHosts>
    <networkRouters>
        <router id="r1" />
        <router id="r2" />
    </networkRouters>
    <networkLinks>
        <link id="l1" tits="dicks">
            <linkRate>10</linkRate>
            <linkDelay>1</linkDelay>
            <bufferSize>100</bufferSize>
            <router1>r1</router1>
            <router2>r2</router2>
        </link>
    </networkLinks>
    <networkFlows>
        <flow id="f1">
            <source>h1</source>
            <destination>h2</destination>
            <flowSize>25</flowSize>
            <flowStartTime>0</flowStartTime>
        </flow>
    </networkFlows>
</Network>

