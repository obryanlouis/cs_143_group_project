<?xml version="1.0" encoding="ISO-8859-1"?>

<Network>
    <snapshotTime>50</snapshotTime>
    <routingTableUpdateTime>5000</routingTableUpdateTime>
    <networkHosts>
        <host id="1" print="1" />
            <!-- S1 -->
        <host id="2" print="1" />
            <!-- S2 -->
        <host id="3" print="1" />
            <!-- S3 -->
        <host id="4" print="1" />
            <!-- T1 -->
        <host id="5" print="1" />
            <!-- T2 -->
        <host id="6" print="1" />
            <!-- T3 -->
    </networkHosts>
    <networkRouters>
        <router id="1" />
            <!-- R1 -->
        <router id="2" />
            <!-- R2 -->
        <router id="3" />
            <!-- R3 -->
        <router id="4" />
            <!-- R4 -->
    </networkRouters>
    <networkLinks>
        <link id="1" print="1">
            <linkRate>10</linkRate>
            <linkDelay>10</linkDelay>
            <bufferSize>128</bufferSize>
            <node1 type="1">1</node1>
            <node2 type="1">2</node2>
        </link>
            <!-- L1 -->
        <link id="2" print="1">
            <linkRate>10</linkRate>
            <linkDelay>10</linkDelay>
            <bufferSize>128</bufferSize>
            <node1 type="1">2</node1>
            <node2 type="1">3</node2>
        </link>
            <!-- L2 -->
        <link id="3" print="1">
            <linkRate>10</linkRate>
            <linkDelay>10</linkDelay>
            <bufferSize>128</bufferSize>
            <node1 type="1">3</node1>
            <node2 type="1">4</node2>
        </link>
            <!-- L3 -->
        <link id="4" print="0">
            <linkRate>12.5</linkRate>
            <linkDelay>10</linkDelay>
            <bufferSize>128</bufferSize>
            <node1 type="0">2</node1>
            <node2 type="1">1</node2>
        </link>
            <!-- link from S2 to R1 -->
        <link id="5" print="0">
            <linkRate>12.5</linkRate>
            <linkDelay>10</linkDelay>
            <bufferSize>128</bufferSize>
            <node1 type="0">1</node1>
            <node2 type="1">1</node2>
        </link>
            <!-- link from S1 to R1 -->
        <link id="6" print="0">
            <linkRate>12.5</linkRate>
            <linkDelay>10</linkDelay>
            <bufferSize>128</bufferSize>
            <node1 type="0">5</node1>
            <node2 type="1">2</node2>
        </link>
            <!-- link from T2 to R2 -->
        <link id="7" print="0">
            <linkRate>12.5</linkRate>
            <linkDelay>10</linkDelay>
            <bufferSize>128</bufferSize>
            <node1 type="0">3</node1>
            <node2 type="1">3</node2>
        </link>
            <!-- link from S3 to R3 -->
        <link id="8" print="0">
            <linkRate>12.5</linkRate>
            <linkDelay>10</linkDelay>
            <bufferSize>128</bufferSize>
            <node1 type="0">4</node1>
            <node2 type="1">4</node2>
        </link>
            <!-- link from T1 to R4 -->
        <link id="9" print="0">
            <linkRate>12.5</linkRate>
            <linkDelay>10</linkDelay>
            <bufferSize>128</bufferSize>
            <node1 type="0">6</node1>
            <node2 type="1">4</node2>
        </link>
            <!-- link from T3 to R4 -->
    </networkLinks>
    <networkFlows>
        <flow id="1" print="1">
            <source>1</source>
            <destination>4</destination>
            <flowSize>35</flowSize>
            <flowStartTime>0.5</flowStartTime>
                <!-- Options: reno, vegas -->
            <congestionAlgorithmType>reno</congestionAlgorithmType>
        </flow>
            <!-- F1 -->
        <flow id="2" print="1">
            <source>2</source>
            <destination>5</destination>
            <flowSize>15</flowSize>
            <flowStartTime>10</flowStartTime>
                <!-- Options: reno, vegas -->
            <congestionAlgorithmType>reno</congestionAlgorithmType>
        </flow>
            <!-- F2 -->
        <flow id="3" print="1">
            <source>3</source>
            <destination>6</destination>
            <flowSize>30</flowSize>
            <flowStartTime>20</flowStartTime>
                <!-- Options: reno, vegas -->
            <congestionAlgorithmType>tahoe</congestionAlgorithmType>
        </flow>
            <!-- F3 -->
    </networkFlows>
</Network>

